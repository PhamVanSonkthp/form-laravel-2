{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "arifhp86/laravel-clear-expired-cache-file": "^1.0",
        "awobaz/compoships": "^2.1",
        "barryvdh/laravel-debugbar": "^3.7",
        "box/spout": "^3.3",
        "doctrine/dbal": "^3.6",
        "fruitcake/laravel-cors": "^2.0",
        "google/auth": "*",
        "guzzlehttp/guzzle": "^7.0.1",
        "intervention/image": "*",
        "kutia-software-company/larafirebase": "^1.3",
        "laravel/framework": "^8.75",
        "laravel/sanctum": "^2.11",
        "laravel/socialite": "^5.5",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^3.4",
        "league/flysystem-aws-s3-v3": "*",
        "maatwebsite/excel": "^3.1",
        "mguinea/laravel-robots": "^3.1",
        "nextapps/laravel-verification-code": "^1.3",
        "owen-it/laravel-auditing": "^13.0",
        "phpoffice/phpspreadsheet": "^1.27",
        "phpoffice/phpword": "^1.0",
        "predis/predis": "^2.2",
        "pusher/pusher-php-server": "^7.0",
        "renatomarinho/laravel-page-speed": "^2.1",
        "spatie/image-optimizer": "^1.7",
        "spatie/laravel-backup": "*",
        "spatie/laravel-health": "*",
        "spatie/laravel-image-optimizer": "^1.6",
        "spatie/laravel-responsecache": "^6.6",
        "spatie/laravel-sitemap": "*",
        "stripe/stripe-php": "^7.114",
        "unisharp/laravel-filemanager": "^2.3",
        "vonage/client": "^3.0"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "larastan/larastan": "*",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^5.10",
        "phpunit/phpunit": "^9.5.10",
        "squizlabs/php_codesniffer": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
